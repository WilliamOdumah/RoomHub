{
    "source": {
      "include": ["src"],
      "exclude": ["./test", "./testing", "./node_modules"],
      "includePattern": ".+\\.js?$",
      "excludePattern": ".*\\.(config|test|spec|example)\\.(js|mjs)$"

    },
    "opts": {
      "destination": "./docs/",
      "recurse": true
    },
    "plugins": ["jsdoc-http-plugin"],
    "templates": {
      "cleverLinks":true,
      "monospaceLinks":true
    }
  }