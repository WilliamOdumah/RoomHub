{
    "source": {
      "include": ["src", "README.md"],
      "exclude": ["./test", "./testing", "./node_modules"],
      "includePattern": ".+\\.js?$",
      "excludePattern": ".*\\.(config|test|spec|example)\\.(js|mjs)$"

    },
    "opts": {
      "destination": "./docs/",
      "recurse": true,
      "title": "RoomHub Documentation"
    },
    "plugins": ["jsdoc-http-plugin", "plugins/markdown"],
    "templates": {
      "cleverLinks":true,
      "monospaceLinks":true
    }
  }